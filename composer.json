{
  "name": "zorgfinder/core",
  "description": "Core plugin for ZorgFinder platform",
  "type": "wordpress-plugin",
  "autoload": {
    "psr-4": {
      "ZorgFinder\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "composer/installers": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
